<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SwipeLayout">
        <attr name="drag_edge" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="top" value="2" />
            <enum name="bottom" value="3" />
        </attr>
        <attr name="horizontalSwipeOffset" format="dimension" />
        <attr name="verticalSwipeOffset" format="dimension" />
        <attr name="show_mode" format="enum">
            <enum name="lay_down" value="0" />
            <enum name="pull_out" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="SlidingUpPanelLayout">
        <attr name="panelHeight" format="dimension" />
        <attr name="shadowHeight" format="dimension" />
        <attr name="paralaxOffset" format="dimension" />
        <attr name="offsetHeight" format="dimension" />
        <attr name="fadeColor" format="color" />
        <attr name="flingVelocity" format="integer" />
        <attr name="dragView" format="reference" />
        <attr name="overlay" format="boolean" />
        <attr name="anchorPoint" format="float" />
    </declare-styleable>
    <declare-styleable name="PagerStrip">
        <attr name="pagerstrip_indicator" format="reference|color" />
        <attr name="pagerstrip_text_color" format="reference" />
        <attr name="pagerstrip_text_selected" format="reference" />
        <attr name="pagerstrip_item_padding" format="dimension" />
        <attr name="pagerstrip_item_divier" format="reference" />
        <attr name="pagerstrip_underline_color" format="reference" />
        <attr name="pagerstrip_scale" format="float" />
        <attr name="pagerstrip_expand" format="boolean" />
    </declare-styleable>
    <declare-styleable name="TitleBar">
        <attr name="titlebar_bg" format="reference|color" />
        <attr name="titlebar_display_home" format="boolean" />
        <attr name="titlebar_title" format="reference" />
        <attr name="titlebar_icon" format="reference" />
        <attr name="titlebar_text_color" format="reference|color" />
        <attr name="titlebar_page_text_color" format="reference|color" />
        <attr name="titlebar_page_title" format="reference" />
        <attr name="titlebar_page_title_visible" format="boolean" />
        <attr name="titlebar_show_divier" format="boolean" />
        <attr name="titlebar_line_color" format="reference" />
        <attr name="titlebar_line_height" format="dimension" />
    </declare-styleable>
    <declare-styleable name="DivideLinearLayout">
        <attr name="divier_show" format="boolean" />
        <attr name="divier_color" format="reference|color" />
        <attr name="divier_padding" format="dimension" />
        <attr name="divier_inner_padding" format="dimension" />
        <attr name="divier_width" format="dimension" />
        <attr name="item_divier_width" format="dimension" />
        <attr name="item_divier_color" format="reference" />
        <attr name="item_divier_padding" format="dimension" />
        <attr name="divier_gravity">
            <flag name="left" value="0x01" />
            <flag name="top" value="0x02" />
            <flag name="right" value="0x04" />
            <flag name="bottom" value="0x08" />
        </attr>
    </declare-styleable>

    <declare-styleable name="WaveView">
        <attr name="wave_color" format="color" />
        <attr name="wave_alpha" format="integer" />
        <attr name="wave_height" format="dimension" />
        <attr name="wave_count" format="integer" />
        <attr name="wave_speed" format="float" />
        <attr name="wave_type" format="enum">
            <enum name="none" value="0" />
            <enum name="retangle" value="1" />
            <enum name="circle" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="FrameView">
        <attr name="fv_progress_info" format="string" />
        <attr name="fv_empty_res" format="reference" />
        <attr name="fv_empty_info" format="string" />
        <attr name="fv_error_res" format="reference" />
        <attr name="fv_error_info" format="string" />
        <attr name="fv_repeat_res" format="reference" />
        <attr name="fv_repeat_info" format="string" />
        <attr name="fv_frame">
            <flag name="container" value="0x0" />
            <flag name="progress" value="0x1" />
            <flag name="empty" value="0x2" />
            <flag name="error" value="0x3" />
            <flag name="repeat" value="0x4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="WheelView">
        <attr name="wv_wheel_mode" format="enum">
            <enum name="set_progress" value="0" />
            <enum name="l_progress" value="1" />
            <enum name="auto_progress" value="2" />
        </attr>
        <attr name="wv_contour_mode" format="enum">
            <enum name="out_contour" value="0" />
            <enum name="all_contour" value="1" />
            <enum name="inner_contour" value="2" />
            <enum name="rvs_contour" value="3" />
        </attr>
        <attr name="wv_text_mode" format="enum">
            <enum name="progress_text" value="0" />
            <enum name="custom_text" value="1" />
            <enum name="empty_text" value="2" />
        </attr>
        <attr name="wv_inner_padding_mode" format="enum">
            <enum name="contour_padding" value="0" />
            <enum name="contour_width" value="1" />
        </attr>
        <attr name="wv_outcontour_color" format="color" />
        <attr name="wv_innercontour_color" format="color" />
        <attr name="wv_outerbounds_color" format="color" />
        <attr name="wv_outcontour_width" format="dimension" />
        <attr name="wv_innercontour_size" format="dimension" />
        <attr name="wv_text" format="reference" />
        <attr name="wv_text_size" format="dimension" />
        <attr name="wv_text_color" format="reference" />
    </declare-styleable>

    <declare-styleable name="FixedGridLayout">
        <attr name="fg_cellwidth" format="dimension" />
        <attr name="fg_cellheight" format="dimension" />
        <attr name="fg_add_anim" format="boolean" />
        <attr name="fg_remove_anim" format="boolean" />
        <attr name="fg_padding" format="dimension" />
        <attr name="rl_raw" format="integer" />
        <attr name="fg_gravity" format="enum">
            <enum name="center" value="0x00" />
            <enum name="left" value="0x01" />
        </attr>
    </declare-styleable>
    <declare-styleable name="RadioGridLayout">
        <attr name="rl_check_res" format="reference" />
        <attr name="rl_default_res" format="reference" />
        <attr name="rl_check_width" format="dimension" />
        <attr name="rl_check_right_padding" format="dimension" />
        <attr name="rl_check_bottom_padding" format="dimension" />
        <attr name="rl_mode" format="enum">
            <enum name="click" value="0" />
            <enum name="single_choose" value="1" />
            <enum name="more_choose" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ImageViewFlat">
        <attr name="flat_color" format="color" />
        <attr name="flat_backgroud" format="color" />
        <attr name="flat_padding" format="dimension" />
        <attr name="flat_duration" format="integer" />
        <attr name="flat_type" format="enum">
            <enum name="none" value="0" />
            <enum name="circle" value="1" />
            <enum name="retangle" value="2" />
        </attr>
        <attr name="flat_click" format="enum">
            <enum name="click" value="0" />
            <enum name="delay_click" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="TabHost">
        <attr name="th_color" format="color" />
        <attr name="th_select_color" format="color" />
        <attr name="th_text_size" format="dimension" />
        <attr name="th_padding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MusicImageView">
        <attr name="mv_padding" format="dimension" />
        <attr name="mv_color" format="color" />
        <attr name="mv_center_padding" format="dimension" />
        <attr name="mv_corner_radius" format="dimension" />
        <attr name="mv_corner_padding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ListIndicator">
        <attr name="li_type" format="enum">
            <enum name="none" value="0x00" />
            <enum name="rectangle" value="0x01" />
            <enum name="circle" value="0x02" />
        </attr>
        <attr name="li_color" format="color" />
        <attr name="li_text_color" format="color" />
        <attr name="li_select_color" format="color" />
        <attr name="li_text_size" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MusicProgressBar">
        <attr name="mb_color" format="color" />
        <attr name="mb_max" format="float" />
        <attr name="mb_progress" format="float" />
        <attr name="mb_rate_color" format="color" />
        <attr name="mb_choose_color" format="color" />
        <attr name="mb_width" format="dimension" />
        <attr name="mb_circle_padding" format="dimension" />
        <attr name="mb_select_radius" format="dimension" />
        <attr name="mb_rate_textsize" format="dimension" />
        <attr name="mb_mode" format="enum">
            <enum name="seek" value="0x00" />
            <enum name="repeat" value="0x01" />
        </attr>
    </declare-styleable>

    <declare-styleable name="DayPickerView">
        <attr name="colorCurrentDay" format="color" />
        <attr name="colorSelectedDayBackground" format="color" />
        <attr name="colorSelectedDayText" format="color" />
        <attr name="colorPreviousDay" format="color" />
        <attr name="colorNormalDay" format="color" />
        <attr name="colorMonthName" format="color" />
        <attr name="colorDayName" format="color" />
        <attr name="textSizeDay" format="dimension" />
        <attr name="textSizeMonth" format="dimension" />
        <attr name="textSizeDayName" format="dimension" />
        <attr name="headerMonthHeight" format="dimension" />
        <attr name="selectedDayRadius" format="dimension" />
        <attr name="calendarHeight" format="dimension" />
        <attr name="enablePreviousDay" format="boolean" />
        <attr name="currentDaySelected" format="boolean" />
        <attr name="drawRoundRect" format="boolean" />
        <attr name="firstMonth" format="enum">
            <enum name="january" value="0" />
            <enum name="february" value="1" />
            <enum name="march" value="2" />
            <enum name="april" value="3" />
            <enum name="may" value="4" />
            <enum name="june" value="5" />
            <enum name="july" value="6" />
            <enum name="august" value="7" />
            <enum name="september" value="8" />
            <enum name="october" value="9" />
            <enum name="november" value="10" />
            <enum name="december" value="11" />
        </attr>
        <attr name="lastMonth" format="enum">
            <enum name="january" value="0" />
            <enum name="february" value="1" />
            <enum name="march" value="2" />
            <enum name="april" value="3" />
            <enum name="may" value="4" />
            <enum name="june" value="5" />
            <enum name="july" value="6" />
            <enum name="august" value="7" />
            <enum name="september" value="8" />
            <enum name="october" value="9" />
            <enum name="november" value="10" />
            <enum name="december" value="11" />
        </attr>
        <attr name="dv_check_mode" format="enum">
            <enum name="check_mode" value="0" />
            <enum name="select_mode" value="1" />
            <enum name="click_mode" value="2" />
        </attr>
        <attr name="enable_select" format="boolean" />
    </declare-styleable>
    <declare-styleable name="FaceView">
        <attr name="fv_color" format="color" />
        <attr name="fv_width" format="dimension" />
        <attr name="fv_ear_color" format="color" />
        <attr name="fv_ear_width" format="dimension" />
        <attr name="fv_inner_color" format="color" />
        <attr name="fv_status" format="enum">
            <enum name="happy" value="0" />
            <enum name="normal" value="1" />
            <enum name="not_happy" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ViewTransilate">
        <!--单个动画执行时间-->
        <attr name="anim_duration" format="integer" />
        <!--群组还原时,以相反方向还原-->
        <attr name="anim_restore_rvs" format="boolean" />
        <!--执行组动画权重 -->
        <attr name="anim_weight" format="integer" />
        <!-- 动画执行方向,如果是translation_x 则执行left/right ,同时开启translation_x/y,则上下左右自动适应 -->
        <attr name="anim_gravity" format="enum">
            <enum name="left" value="0x00" />
            <enum name="top" value="0x01" />
            <enum name="right" value="0x02" />
            <enum name="bottom" value="0x03" />
            <enum name="near" value="0x04" />
        </attr>
        <!-- 动画模式 -->
        <attr name="anim_mode" format="enum">
            <enum name="all" value="0x00" />
            <enum name="own" value="0x01" />
            <enum name="increase" value="0x02" />
            <enum name="all_rvs" value="0x03" />
        </attr>
        <!--动画体系类别,可自由组合-->
        <attr name="anim">
            <flag name="alpha" value="0x01" />
            <flag name="translation_x" value="0x02" />
            <flag name="translation_y" value="0x04" />
            <flag name="scale_x" value="0x08" />
            <flag name="scale_y" value="0x10" />
            <flag name="rotation" value="0x20" />
            <flag name="rotation_x" value="0x40" />
            <flag name="rotation_y" value="0x80" />
        </attr>
        <attr name="alpha_value" format="float" />
        <attr name="scalex_value" format="float" />
        <attr name="scaley_value" format="float" />
        <attr name="rotation_value" format="float" />
        <attr name="rotationx_value" format="float" />
        <attr name="rotationy_value" format="float" />
    </declare-styleable>
</resources>