<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <layer-list>
            <item android:id="@+id/ll_cover_press">
                <shape android:shape="oval" />
            </item>
            <item>
                <bitmap android:src="@drawable/subscribe_mask" />
            </item>
        </layer-list>
    </item>
    <item android:state_pressed="false">
        <layer-list>
            <item android:id="@+id/ll_cover">
                <shape android:shape="oval" />
            </item>
            <item>
                <bitmap android:src="@drawable/subscribe_mask" />
            </item>
        </layer-list>
    </item>
</selector>